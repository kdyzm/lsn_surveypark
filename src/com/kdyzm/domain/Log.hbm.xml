<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "file:///F:/程序/java/Hibernate/dtd文件/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kdyzm.domain.Log" table="logs">
		<id name="logId" type="string" length="32">
			<generator class="uuid"></generator>
		</id>
		<property name="operateParams" type="string" length="500"></property>
		<property name="operateResult" type="string" length="50"></property>
		<property name="operator" type="string" length="50"></property>
		<property name="operatorDate" type="timestamp"></property>
		<property name="operatorName" type="string" length="100"></property>
		<property name="resultMessage" type="string" length="200"></property>
	</class>
</hibernate-mapping>